<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wsdm.produce.mapper.ProduceManageMapper">


    <resultMap type="com.wsdm.produce.domain.ProduceManageVO" id="ProduceManageVoResult">
        <result property="id" column="id"/>
        <result property="produceName" column="produce_name"/>
        <result property="produceNumber" column="produce_number"/>
        <result property="produceModel" column="produce_model"/>
        <result property="speedRatio" column="speed_ratio"/>
        <result property="assembleModality" column="assemble_modality"/>
        <result property="status" column="status"/>
        <result property="createBy" column="create_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>

    <resultMap type="ProduceManage" id="ProduceManageResult">
        <result property="id" column="id"/>
        <result property="produceName" column="produce_name"/>
        <result property="produceNumber" column="produce_number"/>
        <result property="produceModel" column="produce_model"/>
        <result property="speedRatio" column="speed_ratio"/>
        <result property="assembleModality" column="assemble_modality"/>
        <result property="status" column="status"/>
        <result property="createBy" column="create_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>

    <resultMap type="ProduceRef" id="ProduceDetailsResult">
        <result property="id" column="id"/>
        <result property="produceName" column="produce_name"/>
        <result property="produceNumber" column="produce_number"/>
        <result property="produceModel" column="produce_model"/>
        <result property="speedRatio" column="speed_ratio"/>
        <result property="assembleModality" column="assemble_modality"/>
        <result property="status" column="status"/>
        <result property="createBy" column="create_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>

    <sql id="selectProduceManageVo">
        select id,
               produce_name,
               produce_number,
               produce_model,
               speed_ratio,
               assemble_modality,
               status,
               create_by,
               create_time,
               update_by,
               update_time
        from pd_produce_manage
    </sql>

    <select id="selectProduceManageList" parameterType="ProduceManage" resultMap="ProduceManageResult">
        <include refid="selectProduceManageVo"/>
        <where>
            <if test="produceName != null  and produceName != ''">
                and produce_name like concat('%', #{produceName}, '%')
            </if>
            <if test="produceNumber != null  and produceNumber != ''">
                and produce_number = #{produceNumber}
            </if>
            <if test="produceModel != null  and produceModel != ''">
                and produce_model = #{produceModel}
            </if>
            <if test="speedRatio != null  and speedRatio != ''">
                and speed_ratio = #{speedRatio}
            </if>
            <if test="assembleModality != null  and assembleModality != ''">
                and assemble_modality = #{assembleModality}
            </if>
            <if test="status != null  and status != ''">
                and status = #{status}
            </if>
        </where>
    </select>

    <select id="selectProduceManageVOList" parameterType="ProduceManage" resultMap="ProduceManageVoResult">
        select id, produce_name, produce_number, produce_model, speed_ratio, assemble_modality, status
        from pd_produce_manage
        <where>
            <if test="produceName != null  and produceName != ''">
                and produce_name like concat('%', #{produceName}, '%')
            </if>
            <if test="produceNumber != null  and produceNumber != ''">
                and produce_number = #{produceNumber}
            </if>
            <if test="produceModel != null  and produceModel != ''">
                and produce_model = #{produceModel}
            </if>
            <if test="speedRatio != null  and speedRatio != ''">
                and speed_ratio = #{speedRatio}
            </if>
            <if test="assembleModality != null  and assembleModality != ''">
                and assemble_modality = #{assembleModality}
            </if>
            <if test="status != null  and status != ''">
                and status = #{status}
            </if>
        </where>
    </select>

    <select id="selectProduceManageById" parameterType="Long" resultMap="ProduceManageResult">
        <include refid="selectProduceManageVo"/>
        where id = #{id}
    </select>

    <select id="selectProduceManageVoById" parameterType="Long" resultMap="ProduceManageVoResult">
        <include refid="selectProduceManageVo"/>
        where id = #{id}
    </select>

    <insert id="insertProduceManage" parameterType="ProduceManage" useGeneratedKeys="true"
            keyProperty="id">
        insert into pd_produce_manage
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,
            </if>
            <if test="produceName != null">produce_name,
            </if>
            <if test="produceNumber != null">produce_number,
            </if>
            <if test="produceModel != null">produce_model,
            </if>
            <if test="speedRatio != null">speed_ratio,
            </if>
            <if test="assembleModality != null">assemble_modality,
            </if>
            <if test="status != null">status,
            </if>
            <if test="createBy != null">create_by,
            </if>
            <if test="createTime != null">create_time,
            </if>
            <if test="updateBy != null">update_by,
            </if>
            <if test="updateTime != null">update_time,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},
            </if>
            <if test="produceName != null">#{produceName},
            </if>
            <if test="produceNumber != null">#{produceNumber},
            </if>
            <if test="produceModel != null">#{produceModel},
            </if>
            <if test="speedRatio != null">#{speedRatio},
            </if>
            <if test="assembleModality != null">#{assembleModality},
            </if>
            <if test="status != null">#{status},
            </if>
            <if test="createBy != null">#{createBy},
            </if>
            <if test="createTime != null">#{createTime},
            </if>
            <if test="updateBy != null">#{updateBy},
            </if>
            <if test="updateTime != null">#{updateTime},
            </if>
        </trim>
    </insert>

    <insert id="insertProduceDetailsBatch"  parameterType="ProduceRef">
        insert into pd_produce_details (`type`, produce, ref, num) values
        <foreach collection ="list" item="item" index= "index" separator =",">
            (#{item.type}, #{item.produce}, #{item.ref}, #{item.num})
        </foreach>
    </insert>

    <update id="updateProduceManage" parameterType="ProduceManage">
        update pd_produce_manage
        <trim prefix="SET" suffixOverrides=",">
            produce_name = #{produceName},
            produce_number = #{produceNumber},
            produce_model = #{produceModel},
            speed_ratio = #{speedRatio},
            assemble_modality = #{assembleModality},
            <if test="status != null">
                status = #{status},
            </if>
            <if test="createBy != null">
                create_by = #{createBy},
            </if>
            <if test="createTime != null">
                create_time = #{createTime},
            </if>
            <if test="updateBy != null">
                update_by = #{updateBy},
            </if>
            <if test="updateTime != null">
                update_time = #{updateTime},
            </if>
        </trim>
        where id = #{id}
    </update>

    <update id="deleteProduceManageById" parameterType="Long">
        update pd_produce_manage set status = '1'
        where id = #{id}
    </update>

    <update id="deleteProduceManageByIds" parameterType="String">
        update pd_produce_manage set status = '1'  where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </update>

    <delete id="deleteProduceDetailsByProduceNumber" parameterType="String">
        delete from pd_produce_details where produce = #{produceNumber}
    </delete>

    <select id="getProduceNumber" parameterType="String" resultMap="ProduceManageResult">
        <include refid="selectProduceManageVo"/>
        where produce_number = #{produceNumber}
    </select>

    <select id="produceListByProduceNumber" resultMap="ProduceManageResult">
        <include refid="selectProduceManageVo"/>
        where produce_number in
        <foreach item="produceNumber" collection="list" open="(" separator="," close=")">
            #{produceNumber}
        </foreach>
    </select>

</mapper>
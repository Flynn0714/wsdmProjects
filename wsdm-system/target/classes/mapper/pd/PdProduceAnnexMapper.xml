<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wsdm.pd.mapper.PdProduceAnnexMapper">

    <resultMap type="PdProduceAnnex" id="PdProduceAnnexResult">
        <result property="id" column="id"/>
        <result property="produceId" column="produce_id"/>
        <result property="annexName" column="annex_name"/>
        <result property="annexUrl" column="annex_url"/>
        <result property="status" column="status"/>
        <result property="createBy" column="create_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>

    <sql id="selectPdProduceAnnexVo">
        select id,
               produce_id,
               annex_name,
               annex_url,
               status,
               create_by,
               create_time,
               update_by,
               update_time
        from pd_produce_annex
    </sql>

    <select id="selectPdProduceAnnexList" parameterType="PdProduceAnnex" resultMap="PdProduceAnnexResult">
        <include refid="selectPdProduceAnnexVo"/>
        <where>
            <if test="produceId != null  and produceId != ''">
                and produce_id = #{produceId}
            </if>
            <if test="annexName != null  and annexName != ''">
                and annex_name like concat('%', #{annexName}, '%')
            </if>
            <if test="annexUrl != null  and annexUrl != ''">
                and annex_url = #{annexUrl}
            </if>
            <if test="status != null  and status != ''">
                and status = #{status}
            </if>
        </where>
    </select>

    <select id="selectPdProduceAnnexById" parameterType="Long" resultMap="PdProduceAnnexResult">
        <include refid="selectPdProduceAnnexVo"/>
        where id = #{id}
    </select>

    <insert id="insertPdProduceAnnex" parameterType="PdProduceAnnex" useGeneratedKeys="true"
            keyProperty="id">
        insert into pd_produce_annex
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="produceId != null">produce_id,
            </if>
            <if test="annexName != null">annex_name,
            </if>
            <if test="annexUrl != null">annex_url,
            </if>
            <if test="status != null">status,
            </if>
            <if test="createBy != null">create_by,
            </if>
            <if test="createTime != null">create_time,
            </if>
            <if test="updateBy != null">update_by,
            </if>
            <if test="updateTime != null">update_time,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="produceId != null">#{produceId},
            </if>
            <if test="annexName != null">#{annexName},
            </if>
            <if test="annexUrl != null">#{annexUrl},
            </if>
            <if test="status != null">#{status},
            </if>
            <if test="createBy != null">#{createBy},
            </if>
            <if test="createTime != null">#{createTime},
            </if>
            <if test="updateBy != null">#{updateBy},
            </if>
            <if test="updateTime != null">#{updateTime},
            </if>
        </trim>
    </insert>

    <update id="updatePdProduceAnnex" parameterType="PdProduceAnnex">
        update pd_produce_annex
        <trim prefix="SET" suffixOverrides=",">
            <if test="produceId != null">produce_id =
                #{produceId},
            </if>
            <if test="annexName != null">annex_name =
                #{annexName},
            </if>
            <if test="annexUrl != null">annex_url =
                #{annexUrl},
            </if>
            <if test="status != null">status =
                #{status},
            </if>
            <if test="createBy != null">create_by =
                #{createBy},
            </if>
            <if test="createTime != null">create_time =
                #{createTime},
            </if>
            <if test="updateBy != null">update_by =
                #{updateBy},
            </if>
            <if test="updateTime != null">update_time =
                #{updateTime},
            </if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deletePdProduceAnnexById" parameterType="Long">
        delete
        from pd_produce_annex
        where id
                  =
              #{id}
    </delete>

    <delete id="deletePdProduceAnnexByIds" parameterType="String">
        delete from pd_produce_annex where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

</mapper>